<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textEN"
        style="@style/InterMedium.LabelsInterMedium14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/en" />

    <TextView
        android:id="@+id/textES"
        style="@style/InterMedium.LabelsInterMedium14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_default_input"
        app:layout_constraintStart_toEndOf="@id/textEN"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/es" />
</androidx.constraintlayout.widget.ConstraintLayout>
